{
    "name" : "org_heigl/geolocation",
    "type" : "library",
    "description": "Provides a form-element for locating a spot on the earth and providing Latitude/Longitude-values",
    "keywords": ["geolocation", "latitude", "longitude", "earth", "zf3"],
    "homepage": "http://github.com/heiglandreas/OrgHeiglGeolocation",
    "license": "MIT",
    "authors": [{
        "name": "Andreas Heigl",
        "email": "andreas@heigl.org",
        "homepage": "http://andreas.heigl.org",
        "role": "Developer"
    }],
    "require": {
        "php": "^7.0",
        "zendframework/zend-form": "^2.5||^3.0",
        "zendframework/zend-inputfilter": "^2.5||3.0",
        "zendframework/zend-validator": "^2.5||^3.0",
        "zendframework/zend-view": "^2.5||^3.0",
        "zendframework/zend-i18n": "^2.5||^3.0",
        "zendframework/zend-servicemanager": "^2.5||^3.0",
        "zendframework/zend-escaper": "^2.5||^3.0",
        "zendframework/zend-modulemanager": "^2.5||^3.0",
        "zendframework/zend-filter": "^2.7"
    },
    "require-dev" : {
        "mockery/mockery" : "^0.9",
        "phpunit/phpunit" : "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Org_Heigl\\Geolocation\\": "src"
        }
    },
    "autoload-dev" : {
        "psr-4": {
            "Org_Heigl\\GeolocationTest\\": "tests"
        }
    },
    "suggests" : {
        "rwoverdijk/assetmanager" : "Helps providing the assets for tzhis package."
    }
}
